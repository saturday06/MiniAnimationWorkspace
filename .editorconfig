root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 4
indent_style = space
tab_width = 4
trim_trailing_whitespace = true

[*.asmdef]
end_of_line = crlf
trim_trailing_whitespace = false

[*.cs]
end_of_line = crlf

[/Assets/MiniAnimationWorkspace/**.cs]
formatters = uncrustify, generic
